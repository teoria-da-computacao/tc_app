digraph "Autómato que aceita sequencias binárias que têm um número de 0's múltiplo de 3" {
	graph [rankdir=LR]
	node [shape=circle]
	edge [arrowhead=vee arrowsize=1]
	Start [label="" shape=none]
	B
	C
	A
	D [shape=doublecircle]
	Start -> A
	A -> A [label=1]
	A -> B [label=0]
	B -> B [label=1]
	B -> C [label=0]
	C -> C [label=1]
	C -> D [label=0]
	D -> B [label=0]
	D -> D [label=1]
}
